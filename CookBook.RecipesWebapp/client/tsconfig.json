{
  "compilerOptions": {
    "target": "es2016",
    "module": "esnext",
    "lib": [
      "es6",
      "dom",
      "es2016.array.include",
      "es2017.object",
      "esnext.asynciterable",
      "es7",
      "es2019",
    ],
    "noEmit": true,
    "allowJs": false,
    "checkJs": false,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "strictFunctionTypes": false,
    "allowSyntheticDefaultImports": true,
    "alwaysStrict": true,
    "strict": true,
    "strictNullChecks": false,
    "moduleResolution": "node",
    "jsx": "react",
    "rootDirs": ["src"],
    "baseUrl": ".",
    "noImplicitAny": true,
  },
  "include": ["./src/**/*"],
  "exclude": [
    "./node_modules",
    "./build",
    "./lib",
    "./**/__tests__/**",
    "./**/*.test.*",
    "./**/*.spec.*",
  ],
  "files": ["./src/images.d.ts"],
}
